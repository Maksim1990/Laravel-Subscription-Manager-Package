{
    "name": "maksim_n/subscription-manager",
    "description": "Convenient Subscription Manager Package",
    "authors": [
        {
            "name": "Maksim Narushevich",
            "email": "narushevich.maksim@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.23",
        "phpspec/phpspec": "7.1.x-dev"
    },
    "autoload": {
        "psr-4": {
            "MaksimN\\SubscriptionManager\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "MaksimN\\SubscriptionManager\\Providers\\SubscriptionManagerServiceProvider"
            ],
            "aliases": {
                "SubscriptionManager": "MaksimN\\SubscriptionManager\\Facades\\SubscriptionManagerFacade"
            }
        }
    }
}
